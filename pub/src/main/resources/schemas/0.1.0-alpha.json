{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/object1681269416.json",
  "title": "Root",
  "type": "object",
  "required": [
    "version",
    "url",
    "stages"
  ],
  "properties": {
    "version": {
      "$id": "#root/version",
      "title": "Version",
      "type": "string",
      "default": "",
      "examples": [
        "0.1.0-alpha"
      ],
      "pattern": "^.*$"
    },
    "url": {
      "$id": "#root/url",
      "title": "Url",
      "type": "string",
      "default": "",
      "examples": [
        "http://localhost:8080/ping"
      ],
      "pattern": "^.*$"
    },
    "stages": {
      "$id": "#root/stages",
      "title": "Stages",
      "type": "array",
      "default": [],
      "items":{
        "$id": "#root/stages/items",
        "title": "Items",
        "type": "object",
        "required": [
          "target",
          "duration"
        ],
        "properties": {
          "target": {
            "$id": "#root/stages/items/target",
            "title": "Target",
            "type": "integer",
            "examples": [
              10
            ],
            "default": 0
          },
          "duration": {
            "$id": "#root/stages/items/duration",
            "title": "Duration",
            "type": "integer",
            "examples": [
              10
            ],
            "default": 0
          }
        }
      }

    }
  }
}
